<?xml version="1.0" encoding="UTF-8"?>
<exml:class xmlns:exml="http://www.jangaroo.net/exml/0.8"
                xmlns="exml:ext.config">
  <exml:cfg name="catalogObjectVariableName" type="String">
    <exml:description>
      The name of the context variable to be injected to the catalog object property.
      The context value should be a catalog object or an array of them.
    </exml:description>
  </exml:cfg>
  <action/>
</exml:class>
