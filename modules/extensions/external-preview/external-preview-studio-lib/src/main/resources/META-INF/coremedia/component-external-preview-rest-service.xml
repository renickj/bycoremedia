<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="externalPreview" class="com.coremedia.blueprint.studio.rest.externalpreview.ExternalPreviewResource">
    <property name="restUrl" value="${externalpreview.restUrl}" />
    <property name="previewUrl" value="${externalpreview.previewUrl}" />
    <property name="urlPrefix" value="${externalpreview.urlPrefix}" />
  </bean>

</beans>
