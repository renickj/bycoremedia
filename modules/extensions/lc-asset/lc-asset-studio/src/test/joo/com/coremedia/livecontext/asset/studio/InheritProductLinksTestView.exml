<?xml version="1.0" encoding="UTF-8"?>
<exml:component xmlns:exml="http://www.jangaroo.net/exml/0.8"
                xmlns="exml:ext.config"
                xmlns:asset="exml:com.coremedia.livecontext.asset.studio.config"
                xmlns:editor="exml:com.coremedia.cms.editor.sdk.config">

  <exml:constant name="TEST_VIEW_ID" value="viewport"/>
  <viewport>
    <defaults>
      <editor:propertyField bindTo="{config.bindTo}" forceReadOnlyValueExpression="{config.forceReadOnlyValueExpression}"/>
    </defaults>
    <items>
      <asset:commerceFormPanel>
        <asset:additionalToolbarItems>
          <asset:inheritProductListButton bindTo="{config.bindTo}"
                                          forceReadOnlyValueExpression="{config.forceReadOnlyValueExpression}"/>
        </asset:additionalToolbarItems>
      </asset:commerceFormPanel>
    </items>
  </viewport>
</exml:component>
