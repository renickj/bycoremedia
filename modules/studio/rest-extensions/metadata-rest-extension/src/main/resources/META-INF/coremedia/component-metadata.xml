<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="mp3MetaDataResolver" class="com.coremedia.blueprint.studio.rest.Mp3MetadataResolver">
    <property name="name" value="id3"/>
  </bean>
  <bean id="imageMetaDataResolver" class="com.coremedia.blueprint.studio.rest.ImageMetadataResolver">
    <property name="name" value="exif"/>
  </bean>

</beans>