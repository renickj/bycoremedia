    <repository.workflow/>

    <studio.previewUrlPrefix>http://${studio.host}:${PREVIEW_PORT_PREFIX}${PREVIEW_HTTP_PORT_SUFFIX}/blueprint/servlet</studio.previewUrlPrefix>

